<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:padding="2dp" >
     

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="Hemoglobin"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/hemseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/hemtext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:text="0"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="ESR"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/esrseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/esrtext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="0"
                android:layout_marginLeft="10dp"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="WBC"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/wbcseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/wbctext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="0"
                android:layout_marginLeft="10dp"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="Neotrophilis"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/neuseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/neutext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="0"
                android:layout_marginLeft="10dp"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="Lymphocytes"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/lymseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/lymtext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="0"
                android:layout_marginLeft="10dp"
                
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                
                android:layout_marginLeft="120dp"
                android:text="Monocytes"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/monoseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/monotext"
                android:layout_width="70dp"
                android:text="0"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="ASO"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <SeekBar
                android:id="@+id/asoseekBar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@color/WHITE" />

            <TextView
                android:id="@+id/asotext"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="0"
                android:layout_marginLeft="10dp"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="120dp"
                android:text="CRP"
                android:textColor="@color/BLACK"
                android:textSize="25dp" />

            <RadioGroup
                android:id="@+id/gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <RadioButton
                    android:id="@+id/crppositive"
                    android:layout_width="140dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:checked="true"
                    android:text="Positive"
                    android:textSize="25dp" />

                <RadioButton
                    android:id="@+id/crpnegative"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Negative"
                    android:textSize="25dp" />
            </RadioGroup>
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="112dp" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <Button
                android:id="@+id/finish"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_marginLeft="400dp"
                android:layout_marginTop="45dp"
                android:background="@drawable/blackbutton"
                android:text="finish"
                android:textColor="@color/WHITE"
                android:textSize="20dp" />
        </TableRow>
    </TableLayout>

</LinearLayout>